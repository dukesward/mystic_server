  },
  "author": "Duke",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "log4js": "^6.9.1",
    "ngrok": "^5.0.0-beta.2",
    "twilio": "^5.3.6"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/mocha": "^10.0.10",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^22.9.0",
    "concurrently": "^9.1.0",
    "log4j": "^1.0.0",
    "log4js-json-layout": "^2.2.3",
    "module-alias": "^2.2.3",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  },
  "_moduleAliases": {
    "@": "src/",
    "@applications": "src/applications",
